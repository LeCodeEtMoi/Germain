---
import Ex_7_T from '../../components/Ex_7_T.tsx';
import BaseHead from '../../components/BaseHead.astro';
import Header from '../../components/Header.astro';
import Footer from '../../components/Footer.astro';

const title = "";
const description = "Déterminer si deux droites dans l'espace sont confondues, parallèles, sécantes ou non coplanaires.";
---

<html lang="fr">
	<head>
		<BaseHead title={title} description={description} />
	</head>

	<body>
		<Header />
		<main>
			<article>
				<div class="prose">
					<div class="title">
						<h1>{title}</h1>
					</div>
					<Ex_7_T client:load/>
				</div>
			</article>
		</main>
		<Footer />
	</body>
</html>
