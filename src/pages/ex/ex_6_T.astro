---
import Ex_6_T from '../../components/Ex_6_T.tsx';
import BaseHead from '../../components/BaseHead.astro';
import Header from '../../components/Header.astro';
import Footer from '../../components/Footer.astro';

const title = "";
const description = "Un exercice pour déterminer si un point de l'espace appartient à une droite définie par une représentation paramétrique.";
---

<html lang="fr">
	<head>
		<BaseHead title={title} description={description} />
	</head>

	<body>
		<Header />
		<main>
			<article>
				<div class="prose">
					<div class="title">
						<h1>{title}</h1>
					</div>
					<Ex_6_T client:load/>
				</div>
			</article>
		</main>
		<Footer />
	</body>
</html>
